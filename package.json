  "main": "./dist/nordic-holidays.js",
  "module": "./dist/nordic-holidays.mjs",
  "types": "./dist/nordic-holidays.d.ts",
  "exports": {
    ".": {
      "require": "./dist/nordic-holidays.js",
      "import": "./dist/nordic-holidays.mjs"
    }
  },
  "devDependencies": {
    "tsup": "^8.2.4",
    "typescript": "^5.5.4"
  },
  "scripts": {
